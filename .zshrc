ZSH=$HOME/.oh-my-zsh # Path to oh-my-zsh installation.
ZSH_THEME="cobalt2" # Current theme.

COMPLETION_WAITING_DOTS="true"

plugins=(
	git
	git-extras
	brew
	brew-cask
)

# User configuration

source $ZSH/oh-my-zsh.sh


